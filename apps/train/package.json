{
	"name": "@kai/apps-train",
	"main": "index.js",
	"version": "1.0.0",
	"engines": {
		"node": ">=18.0.0",
		"yarn": "please-use-bun",
		"npm": "please-use-bun",
		"bun": ">=1.1.27"
	},
	"scripts": {
		"start:production": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=production expo start -c",
		"start:test": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=test expo start -c",
		"start:development": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=development expo start -c",
		"start:local": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=local expo start -c",
		"android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android -c --android",
		"dev:ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:ios",
		"dev:android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
		"dev:web": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start -c --web",
		"build": "EXPO_USE_METRO_WORKSPACE_ROOT=1 npx expo export -p web",
		"build:android": "eas build -p android --profile preview",
		"build:android:local": "cd android && ./gradlew clean && ./gradlew assembleRelease --stacktrace && cd ..",
		"serve": "npx serve dist --single",
		"clean": "rm -rf .expo node_modules && watchman watch-del-all && rm -rf package-lock.json && npm install",
		"clean:android": "cd android && ./gradlew clean && cd ..",
		"clean:ios": "cd ios && rm -rf Pods && pod install && cd ..",
		"postinstall": "npx tailwindcss -i ./global.css -o ../../node_modules/.cache/nativewind/global.css",
		"ios": "expo run:ios -c --ios",
		"tailwind": "tailwindcss --config ../../packages/tailwind-config/tailwind.config.js",
		"generate:api": "kubb generate",
		"check-format": "biome format .",
		"lint-format": "biome lint .",
		"check-lint": "biome check .",
		"test-all": "bun check-format && bun lint-format && bun check-lint && bun build"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@clerk/clerk-expo": "^2.3.10",
		"@dev-plugins/react-query": "^0.0.6",
		"@expo/metro-config": "^0.18.11",
		"@expo/vector-icons": "^14.0.2",
		"@hookform/resolvers": "^3.9.0",
		"@react-native-async-storage/async-storage": "1.23.1",
		"@react-native-community/netinfo": "^11.4.1",
		"@kai/core-components": "*",
		"@kai/core-hooks": "*",
		"axios": "^1.7.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"expo": "~51.0.28",
		"expo-linear-gradient": "~13.0.2",
		"expo-constants": "~16.0.2",
		"expo-crypto": "~13.0.2",
		"expo-font": "~12.0.9",
		"expo-linking": "~6.3.1",
		"expo-router": "~3.5.23",
		"expo-secure-store": "~13.0.2",
		"expo-splash-screen": "~0.27.5",
		"expo-status-bar": "~1.12.1",
		"expo-system-ui": "~3.0.7",
		"expo-web-browser": "~13.0.3",
		"lucide-react-native": "^0.460.0",
		"nativewind": "^4.1.23",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "0.74.5",
		"react-native-gesture-handler": "~2.16.1",
		"react-native-reanimated": "~3.10.1",
		"react-native-safe-area-context": "4.10.5",
		"react-native-screens": "3.31.1",
		"react-native-web": "~0.19.10",
		"react-native-svg": "^15.9.0",
		"tailwind-merge": "^2.5.4",
		"tailwindcss": "^3.4.14",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@types/jest": "^29.5.12",
		"@types/react": "~18.2.45",
		"@types/react-test-renderer": "^18.0.7",
		"expo-dev-client": "^4.0.29",
		"jest": "^29.2.1",
		"jest-expo": "~51.0.3",
		"metro-minify-esbuild": "^0.2.0",
		"react-native-dotenv": "^3.4.11",
		"react-native-svg-transformer": "^1.5.0",
		"react-test-renderer": "18.2.0",
		"typescript": "~5.3.3"
	},
	"private": true
}
